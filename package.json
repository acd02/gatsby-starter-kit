{
  "name": "gatsby-starter-kit",
  "description": "Gatsby with TypeScript",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run lint:prod && gatsby build",
    "clean": "gatsby clean",
    "develop": "env-cmd -f .env.development gatsby develop",
    "generate": "graphql-codegen",
    "lint": "eslint --config .eslintrc.js ./src --ext .ts,.tsx --fix",
    "lint:styles": "stylelint --fix \"./src/**/*.module.css\"",
    "lint:prod": "eslint --config .eslintrc.prod.js ./src --ext .ts,.tsx --fix",
    "lint-staged": "lint-staged --config .lintstagedrc.json",
    "prettier": "prettier --config .prettierrc.json --write \"src/**/*.{ts,tsx}\"",
    "start": "npm run develop"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.8.3",
    "@babel/plugin-proposal-optional-chaining": "7.8.3",
    "@types/node": "13.1.8",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "@types/react-helmet": "5.0.15",
    "acd-utils": "6.0.0",
    "babel-preset-gatsby": "0.2.27",
    "gatsby": "2.18.25",
    "gatsby-plugin-postcss": "2.1.20",
    "gatsby-plugin-purgecss": "4.0.1",
    "gatsby-plugin-react-helmet": "3.1.21",
    "gatsby-plugin-sass": "2.1.27",
    "gatsby-plugin-typescript": "2.1.26",
    "gatsby-plugin-typescript-checker": "1.1.1",
    "gatsby-source-filesystem": "2.1.46",
    "gatsby-transformer-remark": "5.25.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.11.2",
    "@graphql-codegen/typescript": "1.11.2",
    "@graphql-codegen/typescript-operations": "1.11.2",
    "@typescript-eslint/eslint-plugin": "2.16.0",
    "@typescript-eslint/parser": "2.16.0",
    "classcat": "4.0.2",
    "env-cmd": "10.0.1",
    "eslint-config-prettier": "6.9.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.18.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "eslint-plugin-simple-import-sort": "5.0.0",
    "eslint-plugin-standard": "4.0.1",
    "gatsby-plugin-eslint": "2.0.8",
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "node-sass": "4.13.1",
    "postcss-nesting": "7.0.1",
    "prettier": "1.19.1",
    "stylelint": "13.2.0",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.0.0",
    "tailwindcss": "1.2.0",
    "ts-node": "8.6.2",
    "typescript": "3.7.5"
  }
}
